    vector<vector<int>> transpose(vector<vector<int>>& matrix) 
    {
 
        int n=matrix.size();
        int m=matrix[0].size();
         vector<vector<int>>res (m,vector<int>(n,0)); //This is ver important step in this step suppose we have a metrix of size 2*3 .Now the resultant matrix after transpose will be 3*2.So according ly we create size of that matrix.
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<m;j++)
            {
                res[j][i]=matrix[i][j];
                
                
            }
        }
        return res;
    }
