    string restoreString(string s, vector<int>& indices)
    {
        map<int,char>mp;
        string res;
        for(int i=0;i<indices.size();i++)
        {
            mp[indices[i]]=s[i];
        }
        for(auto itr: mp)
        {
            res.push_back(itr.second);
        }
        return res;
    }
};
